{
    "name": "dminustin/lsne-tags-package",
    "description": "Tags package",
    "keywords": [
        "dminustin",
        "tags-package"
    ],
    "homepage": "https://github.com/dminustin/lsne-tags-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Danila Minustin",
            "email": "danila.minustin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dminustin\\LSNE\\TagsPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dminustin\\LSNE\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dminustin\\LSNE\\TagsPackage\\PackageServiceProvider"
            ],
            "aliases": {
                "CorePackage": "Dminustin\\LSNE\\TagsPackage\\PackageFacade"
            }
        }
    }
}
